<launch>
    
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
        <param name="map_update_interval" value="2.0"/>
        <param name="delta" value="0.05"/>
        <param name="odom_frame" value="odom"/>
        <param name="base_frame" value="base_link"/>
        <param name="laser_frame" value="base_laser"/>
    </node>
    
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find trabalho_slam)/rviz/mapeamento.rviz" />
    
    <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" output="screen" launch-prefix="xterm -e" />

</launch>
